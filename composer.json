{
    "name": "fsi/admin-translatable-bundle",
    "type": "symfony-bundle",
    "description": "Purpose of this bundle is to provide simple translation mechanisms for fsi/admin-bundle",
    "keywords": ["admin", "symfony2", "bundle", "fsi", "translatable"],
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Czocher",
            "email": "wasper@wasper.pl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "^2.6|^3.0",
        "symfony/options-resolver": "^2.6|^3.0",
        "symfony/routing": "^2.4|^3.0",
        "symfony/translation": "^2.6|^3.0",
        "symfony/validator": "^2.5|^3.0",
        "fsi/admin-bundle": "^3.0@dev",
        "fsi/doctrine-extensions-bundle": "^1.1@dev,>=1.1.3",
        "fsi/doctrine-extensions": "^1.1@dev",
        "fsi/datasource-bundle": "^1.3@dev",
        "fsi/datasource": "^1.3@dev"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-bundle": "^1.6",
        "phpspec/phpspec": "^2.4",
        "behat/behat": "^3.3",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.1",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "sensiolabs/behat-page-object-extension": "^2.0",
        "bossa/phpspec2-expect": "^1.0",
        "fsi/resource-repository-bundle": "^1.1|^2.0@dev",
        "egeloen/ckeditor-bundle": "^4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminTranslatableBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\FixturesBundle\\": "features/fixtures/project/src/FSi/FixturesBundle"
        },
        "classmap": [ "features/fixtures/project/app/AppKernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev"
        }
    }
}
